        app:layout_constraintStart_toStartOf="@+id/imageView110"
        app:layout_constraintTop_toTopOf="@+id/imageView110"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/rectangle_35" />

    <ImageView
        android:id="@+id/imageView112"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imageView110"
        app:layout_constraintEnd_toEndOf="@+id/imageView110"
        app:layout_constraintHorizontal_bias="0.531"
        app:layout_constraintStart_toStartOf="@+id/imageView110"
        app:layout_constraintTop_toTopOf="@+id/imageView110"
        app:layout_constraintVertical_bias="0.306"
        app:srcCompat="@drawable/camera" />

    <ImageView
        android:id="@+id/imageView113"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imageView112"
        app:layout_constraintEnd_toEndOf="@+id/imageView111"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="@+id/imageView111"
        app:layout_constraintTop_toTopOf="@+id/imageView112"
        app:layout_constraintVertical_bias="0.258"
